Cell[BoxData[
 RowBox[{
  RowBox[{"OneZero", "[", 
   RowBox[{"n_Integer", "?", "Positive"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "k"}], "}"}], ",", 
    RowBox[{
     RowBox[{"i", "=", 
      RowBox[{"k", "=", "1"}]}], ";", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "<", "n"}], ",", 
       RowBox[{
        RowBox[{"k", "++"}], ";", 
        RowBox[{"i", "=", 
         RowBox[{"i", "+", "k"}]}], ";"}]}], "]"}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", "\[Equal]", "n"}], ",", 
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "=", "0"}]}], "]"}], ";", "\[IndentingNewLine]", "i"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",ExpressionUUID->"2b81c17f-434c-\
